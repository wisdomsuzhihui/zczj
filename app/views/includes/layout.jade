		meta(http-equiv="X-UA-Compatible" content="IE=edge")
		meta(name="viewport", content="width=device-width, user-scalable=no, initial-scale=1.0")
		title #{title} 
		link(href="/libs/css/base.min.css", rel="stylesheet")
